function scramble(str1, str2) {
  const result =[];
  for(let i=0; i<str2.length; i++){
     if(str1.includes(str2[i])){
      result.push(str2[i])
       str1=str1.replace(str2[i],'')
    }else{console.log('not found',str2[i])}
  }  
 

  if(JSON.stringify([...str2].sort()) === JSON.stringify(result.sort()))
  return true; 
  return false;
}
